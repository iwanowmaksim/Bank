<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <SolidColorBrush
        x:Key="BorderBrush"
        Color="Green" />

    <SolidColorBrush
        x:Key="BackgroundBrush"
        Color="White" />

    <SolidColorBrush
        x:Key="WindowBackgroundBrush"
        Color="#373737" />

    <SolidColorBrush
        x:Key="WhiteBrush"
        Color="White" />

    <SolidColorBrush
        x:Key="DarkGrayBrush"
        Color="DarkGray" />

    <SolidColorBrush
        x:Key="CyanBrush"
        Color="Cyan" />

    <SolidColorBrush
        x:Key="ForegroundBrush"
        Color="#373737" />

    <SolidColorBrush
        x:Key="ForegroundBlackBrush"
        Color="Black" />

    <LinearGradientBrush
        x:Key="GradientBrush"
        StartPoint="0,0"
        EndPoint="1,1">
        <GradientBrush.GradientStops>
            <GradientStopCollection>
                <GradientStop
                    Offset="0"
                    Color="LightGray"/>
                <GradientStop
                    Offset="0.2"
                    Color="Silver"/>
                <GradientStop
                    Offset="1"
                    Color="DarkSlateGray"/>
            </GradientStopCollection>
        </GradientBrush.GradientStops>
    </LinearGradientBrush>
</ResourceDictionary>